<?xml version="1.0" encoding="UTF-8" ?>
<phpdocumentor
        configVersion="3"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xmlns="https://www.phpdoc.org"
        xsi:noNamespaceSchemaLocation="https://docs.phpdoc.org/3.0/xml/phpdoc.xsd">
    <title>MHD Panel Dokumentace</title>
    <paths>
        <output>docs/api/php</output>
        <cache>docs/api/php/cache</cache>
    </paths>
    <version number="3.0.0">
        <api>
            <source dsn=".">
                <path>.</path>
            </source>
            <ignore>
                <path>docs/**/*</path>
                <path>vendor/**/*</path>
            </ignore>
        </api>
    </version>
</phpdocumentor>
